{
  "name": "free-nextjs-admin-dashboard",
  "version": "2.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true});\" && next dev -p 3000",
    "build": "next build",
    "build:prod": "npx prisma migrate deploy && next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 restart tailadmin --update-env",
    "pm2:save": "pm2 save",
    "deploy:win": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-windows.ps1",
    "deploy:ubuntu": "bash ./scripts/deploy-ubuntu.sh"
  },
